{
    "name": "dm-macro",
    "displayName": "dm-macro",
    "description": "Datamine Macro Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dm-macro",
            "aliases": ["dm-macro", "dm-macro"],
            "extensions": [".mac"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dm-macro",
            "scopeName": "source.dm-macro",
            "path": "./syntaxes/dm-macro.tmLanguage.json"
        }],
        "snippets":[
            {
               "language":"dm-macro",
               "path":"./snippets/dm-macro.code-snippets"
            }
         ]
    }
}